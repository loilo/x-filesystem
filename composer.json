{
    "name": "loilo/x-filesystem",
    "type": "library",
    "version": "1.0.0",
    "description": "An extension to Symfony's Filesystem Component, able to read and write PHP/YAML/JSON/CSV files",
    "authors": [
        {
            "name": "Florian Reuschel",
            "email": "florian@loilo.de"
        }
    ],
    "require": {
        "ext-mbstring": "*",
        "php": ">= 7.1",
        "symfony/filesystem": "^3|^4",
        "symfony/yaml": "^3|^4"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "squizlabs/php_codesniffer": "^3.4",
        "symfony/process": "^4.3"
    },
    "autoload": {
        "psr-4": {
            "Loilo\\XFilesystem\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Loilo\\XFilesystem\\Test\\": "test/"
        }
    },
    "config": {
        "preferred-install": "dist"
    }
}
